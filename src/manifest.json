{
	"manifest_version": 2,
	"name": "newtab-bookmarks",
	"version": "1.0.0",
	"author": "Tom",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"chrome_settings_overrides": {
		"homepage": "index.html"
	},
	"description": "New tab replacement",
	"permissions": [
		"bookmarks",
		"storage"
	],
	"options_ui": {
		"page": "settings.html",
		"open_in_tab": true,
		"browser_style": true
	},
	"icons": {
		"48": "assets/icon.png",
		"96": "assets/icon.png"
	}
}
